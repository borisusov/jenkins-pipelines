pipeline {
    agent any;
    options {
        copyArtifactPermission('master');
    }

    tools {
        maven 'mvn_3.6.3' 
    }
    
    stages {
        stage('2 - Build') {
            steps { 
                sh "mvn clean install"
                sh "ls -la" 
                sh "pwd"
                
            }
        }
    
    }

    post {
        always {
            archiveArtifacts artifacts: '/root/target/*.war', fingerprint: true
        }
    }

}